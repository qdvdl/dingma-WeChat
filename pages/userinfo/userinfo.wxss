.userinfolist{
  width: 690rpx;
  margin: 0 auto;
}
/* 设置按钮 */
.seticon{
  position: absolute;
  right: 30rpx;
  top: 44rpx;
}
.operation{
  background:#ffffff;
  position: relative;
  z-index: 2;
  width: 690rpx;
  margin: 0 auto;
  border-radius:10px;
}
.operation-list{
  padding: 0 20rpx 0 29rpx;
}
.operation-list view.item{
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #EEEEEE;
  position: relative;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
}
.operation-list view.noborder{
  border:none;
}

.foo-icon{
  position: absolute;
  right: 0;
  top: 0;
}
.operation-list view.item .textimg{
  float: right;
  padding-right: 71rpx;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.operation-list view.item .textimg.img image{
  width:80rpx;
  height:80rpx;
  border-radius:50%;
  position: relative;
  top: 10rpx;
  float: left;
}
.inputbox{
  width:504rpx; 
  height:82rpx;
  margin: 0 auto;
  padding: 34rpx 0;
}
