/* pages/mycode/mycode.wxss */
.imgbox{
  width: 750rpx;
  height: 100vh;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
  /* background: red; */
}
.imgbox image{
  width: 100%;
  height: 1220rpx;
}
.btn-list{
  position: absolute;
  bottom: 60rpx;
  left: 0;
  width: 100%;
  text-align: center;

}
.btn-list button{
  width: 150rpx;
  margin-right: 30rpx;
  background: rgba(0, 0, 0, 0)
}
.zz{
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9;
  width: 100vw;
  height: 100vh;
  background: #ffffff;
}
.bbb{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}