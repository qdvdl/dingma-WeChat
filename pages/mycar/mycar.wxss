.main{
  background: #f7f7f7;
  height: 100vh;
}
.bg{
  height: 332rpx;
  margin-bottom: 30rpx;
  position: relative;
}
.bg image{
  width: 100%;
  height: 100%;
}

.text-price{
  position: absolute;
  left: 0;
  top: 104rpx;
  z-index: 2;
  width: 100%;
  text-align: center;

}
.text-price .text{
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:500;
  color:rgba(247,247,247,1);
  margin-bottom: 50rpx;
}   
.text-price .price{
  font-size:60rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(247,247,247,1);
}  
.text-price .price text{
   font-size:47rpx;
}                        
/* 车辆信息 */
.infomain{
  background: #FFFFFF;
  width: 690rpx;
  margin: 0 auto 20rpx;
  border-radius:10rpx;
}
.info-text{
  padding:30rpx 30rpx 0 30rpx;
  
}
.info-text .name{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
  position: relative;
  margin-bottom: 19rpx;
}
.info-text .name text{
  margin-right: 24rpx;
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(51,51,51,1);
}
.state{
  position: absolute;
  right: 0;
  top: 0;
  font-size:28rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(238,124,53,1);
}
.info-text .price{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
  margin-bottom: 20rpx;
}
.info-text text.s{
  font-size:30rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,77,48,1);
  margin-left: 50rpx;
}
.info-text text.t{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:bold;
  color:rgba(255,77,48,1);
}
.info-text .price1{
  font-size:24rpx;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  margin-bottom: 38rpx;
}
.info-text .price1 text{
  color:rgba(255,77,48,1);
}
.time{
  display: flex;
  justify-content: space-between;
  height: 90rpx;
  line-height: 90rpx; 
  border-top:1px solid #EEEEEE; 
  font-size:24rpx;
  font-weight:400;
  font-family:PingFang SC;
}
.time .t{
color:rgba(153,153,153,1);
}
.time .n{
  color:rgba(51,51,51,1);
}

